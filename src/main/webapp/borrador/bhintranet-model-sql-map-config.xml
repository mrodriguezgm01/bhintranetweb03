        cacheModelsEnabled="true"
        enhancementEnabled="true"
        useStatementNamespaces="true"
        maxSessions="64"
        maxTransactions="8"
        maxRequests="128"/>
	<!-- MAPAS DE INTRANET -->

	<sqlMap resource="/pe/bhintranet/model/dao/ibatis/maps/usuario_SqlMap.xml"/>	                    
</sqlMapConfig>